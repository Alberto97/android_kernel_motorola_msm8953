&pm8994_mpps {
	mpp@a100 { /* MPP 2 - MOD_MPHY_I2S_EN_N */
		qcom,mode = <1>;	/* Digital output */
		qcom,vin-sel = <2>;	/* 1.8 */
		qcom,src-sel = <0>;	/* GPIO */
		qcom,master-en = <1>;	/* Enable GPIO */
		qcom,invert = <1>;	/* Output high (disabled) */
		status = "okay";
	};

	mpp@a300 { /* MPP 4 - USB_AUD_DET_N */
		qcom,mode = <1>;	/* Digital output */
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
		qcom,invert = <1>;	/* Output high disabled */
		qcom,master-en = <1>;
		status = "okay";
	};

	mpp@a400 { /* MPP 5 - CCP_FAULT_EN */
		status = "okay";
		qcom,mode = <1>;		/* Digital Output */
		qcom,vin-sel = <2>;		/* S4 1.8V */
		qcom,src-sel = <0>;		/* Constant */
		qcom,master-en = <1>;		/* Enable GPIO */
	};
	mpp@a500 { /* MPP 6 - SOL or CHARGING LED */
		status = "okay";
		qcom,mode = <6>;	/* QPNP_PIN_MODE_SINK */
	};

	mpp@a600 { /* MPP 7 - PCM_THERM */
		status = "okay";
		qcom,mode = <4>; /* AIN input */
		qcom,ain-route = <2>; /* AMUX 7 */
		qcom,master-en = <1>; /* Enable MPP */
	};

};

&pmi8994_mpps {
	mpp@a000 { /* MPP 1 - PCHG_THERM */
		status = "okay";
		qcom,mode = <4>; /* AIN input */
		qcom,ain-route = <0>; /* AMUX 5 */
		qcom,master-en = <1>; /* Enable MPP */
	};
};

