&sdcc1_ice {
        status = "ok";
};

/ {
	aliases {
		/delete-property/ i2c12;
		i2c5 = &i2c_5;
		spi3 = &spi_3;
		spi9 = &spi_9;
		spi12 = &spi_12;
		uart0 = &blsp1_uart2;
		uart5 = &blsp2_uart5;
	};

	reserved-memory {
		ramoops_mem: ramoops_mem_region {
			no-map;
			reg = <0x0 0xaf000000 0x0 0x80000>;
			label = "ramoops_mem";
		};
	};
};

