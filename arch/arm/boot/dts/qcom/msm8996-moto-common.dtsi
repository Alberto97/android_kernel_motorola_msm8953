	qcom,use-bam;
	spi-max-frequency = <50000000>;
	status = "ok";

	florida: florida@0 {
		compatible = "wlf,wm5110";
		spi-max-frequency = <26000000>;
		reg = <0x0>;
		interrupt-parent = <&tlmm>;
		interrupts = <54 0>;
		irq-base = <800>;
		gpio-base = <200>;
		wlf,reset = <&tlmm 64 0>;
		wlf,irq = <&tlmm 54 0>;
		wlf,out-mono = <0 1 0 0 0 0>;
		wlf,micbias1 = <1800 1 1 0 0>;
		wlf,micbias2 = <1800 1 1 0 0>;
		wlf,micbias3 = <1800 1 1 0 0>;
		wlf,vib_pwm_gpio_ctrl = <0xC02>;
		vreg-arizona1p2-supply = <&dummy_vreg>;
		pinctrl-names = "default";
		pinctrl-0 = <&florida_irq_default &florida_reset_default>;

		AVDD-supply = <&pm8994_s4>;
		DCVDD-supply = <&pm8994_s4>;
		DBVDD1-supply = <&pm8994_s4>;
		DBVDD2-supply = <&pm8994_s4>;
		DBVDD3-supply = <&pm8994_s4>;
		CPVDD-supply = <&pm8994_s4>;
		SPKVDDL-supply = <&dummy_vreg>;
		SPKVDDR-supply = <&dummy_vreg>;

		gpio-controller;
		#gpio-cells = <2>;

		wlf,gpio-defaults = <0x00000004
				0xffffffff
				0x00010000
				0x00000004
				0xffffffff>;

		adsps {
			#address-cells = <1>;
			#size-cells = <0>;
			adsp@1200 {
				reg = <0x1200>;
				firmware {
					frontend {
						wlf,wmfw-file = "aov-frontend";
