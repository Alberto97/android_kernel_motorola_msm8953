/dts-v1/;


#include "msm8953-albus.dtsi"


/ {
	qcom,board-id = <0x48 0x81A1>;
};

&soc {
	sound {
		qcom,model = "msm8952-marley-cardp1a";
		qcom,l35_cpu_dai_name = "marley-aif2";
	};

	spi@7af7000 {
		marley@0 {
			wlf,micd-configs = <0 3 0
					    0x2000 2 1>;
		};
	};
};

&spmi_bus {
	qcom,pmi8950@3 {
		qcom,leds@d300 {
			/delete-property/ qcom,self-check-enabled;
		};
	};
};
