&tlmm {
	i2c_5 {
		i2c_5_active: i2c_5_active {
			i2c_5 {
				pins = "gpio83", "gpio84";
				function = "blsp_i2c5";
				drive-strength = <2>;
				bias-disable;
			};
		};

		i2c_5_sleep: i2c_5_sleep {
			i2c_5 {
				pins = "gpio83", "gpio84";
				function = "blsp_i2c5";
				drive-strength = <2>;
				bias-pull-up;
			};
		};
	};

	i2c_8 {
		i2c_8_active: i2c_8_active {
			i2c_8 {
				pins = "gpio6", "gpio7";
				function = "blsp_i2c8";
				drive-strength = <2>;
				bias-disable;
			};
		};

		i2c_8_sleep: i2c_8_sleep {
			i2c_8 {
				pins = "gpio6", "gpio7";
				function = "blsp_i2c8";
				drive-strength = <2>;
				bias-pull-up;
			};
		};
	};

	spi_12 {
		spi_12_active: spi_12_active {
			spi_12 {
				pins = "gpio85", "gpio86", "gpio87", "gpio88";
				function = "blsp_spi12";
				drive-strength = <6>;
				bias-disable;
			};
		};

		spi_12_sleep: spi_12_sleep {
			spi_12 {
				pins = "gpio85", "gpio86", "gpio87", "gpio88";
				function = "blsp_spi12";
				drive-strength = <6>;
				bias-disable;
			};
		};
	};

};
