#include "msm8996-v3.dtsi"
#include "msm8996-moto-common.dtsi"

/ {
	model = "Sheridan";
	compatible = "qcom,msm8996-sheridan", "qcom,msm8996-moto",
			"qcom,msm8996";
};

&drv2605 {
	effects_library = <6>;
	rated_voltage = <0x2d>;         /* 0.96v */
	overdrive_voltage = <0x89>;     /* 3v */
	rtp_overdrive_voltage = <0x89>; /* 3v */
	lra_drive;
};
